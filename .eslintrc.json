{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "prettier/prettier": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-useless-constructor": "off",
    "no-empty-function": "off"
  }
}
